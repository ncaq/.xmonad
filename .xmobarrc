Config { font = "xft:Ricty"
       , bgColor = "#002b36"
       , fgColor = "#839496"
       , position = Static {xpos = 100,ypos = 0,width = 1820,height = 16}
       , lowerOnStart = False
       , commands = [ Run StdinReader
                    , Run Date "%m/%d/%a %H-%M-%S" "date" 10
                    , Run Cpu ["-t","total=<total>%"] 100
                    , Run Memory ["-t","used=<used>"] 100
                    ]
       , sepChar = "%"
       , alignSep = "{}"
       , template = "%StdinReader% {} %cpu% %memory% <fc=#b58900>%date%</fc>"
       }
