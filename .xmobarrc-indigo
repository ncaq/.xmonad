Config
{ font = "xft:monospace:antialias=true"
, bgColor = "#002b36"
, fgColor = "#93a1a1"
, position = TopW L 90
, lowerOnStart = False
, commands =
  [ Run StdinReader
  , Run Cpu [] 100
  , Run CpuFreq ["-t", "Freq: <max>GHz", "--ddigits", "2"] 100
  , Run Memory ["-t", "Mem: <used>M"] 100
  , Run Swap ["-t", "Swap: <used>M"] 100
  , Run DiskIO [("/", "IO: <read> <write>")] [] 100
  , Run DynNetwork ["-t", "Net: <rx>KB|<tx>KB"] 100
  , Run MultiCoreTemp [] 100
  , Run BatteryP ["BAT0"] ["-t", "Bat: <watts> <left>%"] 600
  , Run DateZone "%F%a%T" "ja_JP.utf8" "Japan" "date" 10
  ]
, sepChar = "%"
, alignSep = "{}"
, template = "%StdinReader% {} %cpu%, %cpufreq%, %memory%, %swap%, %diskio%, %dynnetwork%, %multicoretemp%, %battery%, %date%"
}
